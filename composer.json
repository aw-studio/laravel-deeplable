{
    "name": "aw-studio/laravel-deeplable",
    "license": "MIT",
    "authors": [
        {
            "name": "jannescb",
            "email": "jannes@aw-studio.de"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "autoload": {
        "psr-4": {
            "AwStudio\\Deeplable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "astrotomic/laravel-translatable": "^11.0",
        "phpunit/phpunit": "^9.4|^10.0",
        "mockery/mockery": "^1.4",
        "illuminate/testing": "^8.0|^9.0|^10.0|^11.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AwStudio\\Deeplable\\DeeplableServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
